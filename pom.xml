        <!-- 生成sources源码包的插件 -->  
        <plugin>  
            <artifactId>maven-source-plugin</artifactId>  
            <version>2.4</version>  
            <configuration>  
                <attach>true</attach>  
            </configuration>  
            <executions>  
                <execution>  
                    <phase>package</phase>  
                    <goals>  
                        <goal>jar-no-fork</goal>  
                    </goals>  
                </execution>  
            </executions>  
        </plugin>
